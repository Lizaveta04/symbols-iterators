{
  "parser": "@babel/eslint-parser",
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "func-names": "off",
    "no-plusplus": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}
